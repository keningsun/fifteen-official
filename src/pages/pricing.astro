---
import WebflowLayout from '../layouts/WebflowLayout.astro';
import headHtml from '../content/webflow/heads/pricing.html?raw';
import htmlAttrsMap from '../content/webflow/attrs.json';
import bodyAttrsMap from '../content/webflow/body-attrs.json';

import PricingPre from '../sections/pricing/PricingPre.astro';
import PricingMainOpen from '../sections/pricing/PricingMainOpen.astro';
import Section01SectionHeader46 from '../sections/pricing/Section01SectionHeader46.astro';
import Section02SectionPricing19 from '../sections/pricing/Section02SectionPricing19.astro';
import Section03SectionHeader46 from '../sections/pricing/Section03SectionHeader46.astro';
import PricingMainClose from '../sections/pricing/PricingMainClose.astro';
import PricingPost from '../sections/pricing/PricingPost.astro';

const key = 'pricing';
---
<WebflowLayout
  htmlAttrs={htmlAttrsMap[key]}
  bodyAttrs={bodyAttrsMap[key]}
  headHtml={headHtml}
>
  <PricingPre />
  <PricingMainOpen />
  <Section01SectionHeader46 />
  <Section02SectionPricing19 />
  <Section03SectionHeader46 />
  <PricingMainClose />
  <PricingPost />
</WebflowLayout>
